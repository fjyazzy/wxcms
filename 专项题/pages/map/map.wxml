<view class='view'>
  <map longitude="{{longitude}}" latitude="{{latitude}}" markers="{{markers}}" covers="{{covers}}" show-location>
    <cover-view>
    经度：{{longitude}}
    </cover-view>
     <cover-view>
    纬度：{{latitude}}
    </cover-view>
    <cover-view>
    速度：{{speed}}
    </cover-view>
    <cover-view>
    精度：{{accuracy}}
    </cover-view>
  </map>
</view>