<view class='contentBottom'>
  <swiper style="width: 100%;" 
  autoplay="true" circular="true" 
  interval="3000"  indicator-dots="true" 
  display-multiple-items='1' >
    <view>
      <block wx:for-index="idx" wx:key="index" wx:for='{{imgList}}'>
        <swiper-item>
          <view>
            <navigator class='one' url="{{item.pagePath}}">
            <image src="{{item.imagePath}}" 
            style="width:100%" mode ="widthFix" >
            </image>
            </navigator>
          </view>
        </swiper-item>
      </block>          
    </view>
  </swiper>
</view>

<view class="Full">  
   <navigator url="/pages/map/map">
    <image src='../../images/4.jpg'></image>
   </navigator>  

   <navigator url="/pages/dcwj/dcwj">
    <image src='../../images/3.jpg'></image>
   </navigator>  
 </view>
 