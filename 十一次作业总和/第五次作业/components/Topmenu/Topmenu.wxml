<!--Topmenu/Topmenu.wxml-->
<view class="page-section">
    <movable-area>
      <movable-view direction="horizontal">
      <view wx:if="{{uname.length<2 }}">游客</view>
      <view wx:else="{{uname.length>=2 }}" >{{uname}}</view>-
      <navigator wx:for="{{TopMenuList}}" wx:key="index" url="{{item.pagePath}}&title={{item.text}}">{{item.text}}-</navigator>
      </movable-view>
    </movable-area>
</view>
