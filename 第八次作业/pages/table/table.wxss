/* pages/table/table.wxss */
.table {
  margin: 1px;
}
.tr {
  display: flex;
  margin: 0px;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.th {
  width: 16%;
  border: 1rpx solid #3366FF;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
.td {
  width:16%;
  height: 100%;
  border: 1rpx solid #3366FF;
  justify-content: center;
  text-align: center;
  font-size:25rpx;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
